<resources>

    <style name="MenuLabelsStyle">
        <item name="android:background">@drawable/fab_label_background</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">16sp</item>
        <item name="android:maxLines">2</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="MenuButtonsStyle">
        <item name="fab_size">mini</item>
        <item name="fab_showAnimation">@animator/jump_from_down</item>
        <item name="fab_hideAnimation">@animator/jump_to_down</item>
        <item name="fab_shadowColor">#444444</item>
        <item name="fab_colorNormal">#00ff00</item>
        <item name="fab_colorPressed">#008000</item>
    </style>

</resources>
