<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomProgressBar">
        <attr name="progressColor" format="color" />
        <attr name="progressDescColor" format="color" />
        <attr name="progressRadius" format="dimension" />
        <attr name="max" format="integer" />
        <attr name="progress" format="integer" />
        <attr name="borderWidth" format="dimension" />
        <attr name="progressDesc" format="string" />
        <attr name="isShowDesc" format="boolean" />
    </declare-styleable>

    <declare-styleable name="GalleryView">
        <attr name="animDuration" format="integer" />
        <attr name="saveText" format="string" />
        <attr name="saveTextSize" format="dimension" />
        <attr name="saveTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="FramedGridView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>
</resources>